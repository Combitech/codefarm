    "koa": "^2.13.0",
    "koa-basic-auth": "^4.0.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^4.0.1",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-jwt": "^4.0.0",
    "koa-route": "^3.2.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
