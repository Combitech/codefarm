{
  "name": "ui",
  "version": "0.0.0",
  "description": "User Interface Service",
  "main": "index.js",
  "scripts": {
    "compile": "echo 'No compile needed'",
    "compile-client": "NODE_ENV=production webpack --config ./client/webpack.config.js",
    "compile-client-dev": "webpack --env.dev --config ./client/webpack.config.js",
    "build": "../../bs/build.sh",
    "extract": "../../bs/extract.sh",
    "load": "../../bs/load.sh",
    "unload": "../../bs/unload.sh",
    "shell": "../../bs/run_cmd.sh bash",
    "start": "../../bs/start.sh --expose 19898 -p 19898:19898 --net=\"host\" -d",
    "prestart-dev": "rm -f client/static/bundle.js",
    "start-dev": "node --harmony_async_await . --bus amqp://localhost/codefarm",
    "start-rel": "NODE_ENV=production node --harmony_async_await . --bus amqp://localhost/codefarm",
    "stop": "../../bs/stop.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "git ls-files \"*.js\" | xargs eslint",
    "install": "if [ -n \"$gitroot\" ]; then $gitroot/src/bs/linklibs.sh; else ../../bs/linklibs.sh; fi",
    "deepinstall": "if [ -n \"$gitroot\" ]; then FORCE_INSTALL_LIBS=1 $gitroot/src/bs/linklibs.sh; else FORCE_INSTALL_LIBS=1 ../../bs/linklibs.sh; fi"
  },
  "author": "Marcus Christensson <marcus.christensson@combitech.se>",
  "license": "MIT",
  "engines": {
    "node": ">= 7.0.0"
  },
  "libraries": [
    "log",
    "service",
    "web",
    "msgbus",
    "misc",
    "servicecom",
    "singleton"
  ],
  "dependencies": {
    "api.io": "^0.5.1",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-async-to-module-method": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "bluebird": "^3.4.6",
    "cheser-icon-theme": "https://github.com/mattiasrunge/cheser-icon-theme",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.0",
    "cytoscape": "^2.7.11",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "flexboxgrid": "^6.3.1",
    "font-roboto-local": "^1.0.1",
    "immutability-helper": "^2.1.0",
    "immutable": "^3.8.1",
    "koa-proxies": "^0.3.0",
    "koa-webpack-dev-middleware": "^1.3.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.16.0",
    "node-sass": "^4.1.1",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.0",
    "react-flexbox-grid": "^0.10.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-router": "^3.0.0",
    "react-svg-loader": "^1.1.1",
    "react-toolbox": "^1.3.0",
    "rxjs": "^5.2.0",
    "sass-loader": "^6.0.2",
    "sift": "^3.2.6",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.8.4",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "yargs": "^6.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^3.7.0"
  }
}
