{
  "name": "ui",
  "version": "0.0.0",
  "description": "User Interface Service",
  "main": "index.js",
  "scripts": {
    "compile": "echo 'No compile needed'",
    "compile-client": "NODE_ENV=production webpack --config ./client/webpack.config.js",
    "compile-client-dev": "webpack --env.dev --config ./client/webpack.config.js",
    "build": "../../bs/build.sh",
    "extract": "../../bs/extract.sh",
    "load": "../../bs/load.sh",
    "unload": "../../bs/unload.sh",
    "shell": "../../bs/run_cmd.sh bash",
    "start": "../../bs/start.sh --expose 19898 -p 19898:19898 --net=\"host\" -d",
    "prestart-dev": "rm -f client/static/bundle.*",
    "start-dev": "node . --bus amqp://localhost/codefarm",
    "start-rel": "NODE_ENV=production node . --bus amqp://localhost/codefarm",
    "stop": "../../bs/stop.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "git ls-files \"*.js\" | xargs eslint",
    "install": "if [ -n \"$gitroot\" ]; then $gitroot/src/bs/linklibs.sh; else ../../bs/linklibs.sh; fi",
    "deepinstall": "if [ -n \"$gitroot\" ]; then FORCE_INSTALL_LIBS=1 $gitroot/src/bs/linklibs.sh; else FORCE_INSTALL_LIBS=1 ../../bs/linklibs.sh; fi"
  },
  "author": "Marcus Christensson <marcus.christensson@combitech.se>",
  "license": "MIT",
  "engines": {
    "node": ">= 7.0.0"
  },
  "libraries": [
    "auth",
    "log",
    "service",
    "web",
    "msgbus",
    "misc",
    "servicecom",
    "singleton",
    "loglib",
    "backend"
  ],
  "dependencies": {
    "api.io": "^1.0.3",
    "array-to-sentence": "^2.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bluebird": "^3.7.2",
    "cheser-icon-theme": "https://github.com/Combitech/cheser-icon-theme",
    "classnames": "^2.2.6",
    "clone": "^2.1.2",
    "cookie": "^0.4.1",
    "css-loader": "^3.6.0",
    "cytoscape": "^3.15.1",
    "domurl": "^2.3.4",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^4.3.0",
    "flexboxgrid": "^6.3.1",
    "font-roboto-local": "^1.0.1",
    "fs-extra-promise": "^1.0.1",
    "imagemin": "^6.1.0",
    "img-loader": "^3.0.1",
    "immutability-helper": "^3.1.1",
    "immutable": "^3.8.2",
    "jsx-control-statements": "^3.2.8",
    "koa": "^2.13.0",
    "koa-proxies": "^0.11.0",
    "material-design-icons": "^3.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-flexbox-grid": "^2.1.2",
    "react-immutable-proptypes": "^2.2.0",
    "react-router": "5",
    "react-svg-loader": "^3.0.3",
    "react-toolbox": "^2.0.0-beta.13",
    "react-transition-group": "^2.2.0",
    "recharts": "^1.8.5",
    "rxjs": "^6.5.5",
    "sass-loader": "^8.0.2",
    "sift": "^13.1.10",
    "sillyname": "^0.1.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
