{
  "name": "flowctrl",
  "version": "0.0.0",
  "description": "Central Flow Contrl Service",
  "main": "index.js",
  "scripts": {
    "compile": "echo 'Nothing to compile'",
    "build": "../../bs/build.sh",
    "extract": "../../bs/extract.sh",
    "load": "../../bs/load.sh",
    "unload": "../../bs/unload.sh",
    "shell": "../../bs/run_cmd.sh bash",
    "start": "../../bs/start.sh --expose 19191 -p 19191:19191 -d",
    "start-dev": "node --harmony_async_await . --bus amqp://localhost/codefarm",
    "start-rel": "npm run start-dev",
    "stop": "../../bs/stop.sh",
    "test": "npm run compile && mocha --harmony_async_await ./test",
    "lint": "git ls-files \"*.js\" | xargs eslint",
    "install": "if [ -n \"$gitroot\" ]; then $gitroot/src/bs/linklibs.sh; else ../../bs/linklibs.sh; fi",
    "deepinstall": "if [ -n \"$gitroot\" ]; then FORCE_INSTALL_LIBS=1 $gitroot/src/bs/linklibs.sh; else FORCE_INSTALL_LIBS=1 ../../bs/linklibs.sh; fi"
  },
  "author": "Mattias Runge <mattias.runge@combitech.se>",
  "license": "MIT",
  "engines": {
    "node": ">= 7.0.0"
  },
  "libraries": [
    "database",
    "typelib",
    "msgbus",
    "emitter",
    "log",
    "service",
    "misc",
    "ssh",
    "web",
    "testsupport",
    "servicecom",
    "singleton"
  ],
  "dependencies": {
    "async-busboy": "^0.3.3",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.0",
    "chai": "^3.5.0",
    "eslint": "^3.18.0",
    "get-port": "^3.0.0",
    "mocha": "^3.1.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0"
  }
}
