{
  "name": "mgmt",
  "version": "0.0.0",
  "description": "Central Management Service",
  "scripts": {
    "compile": "echo 'No compile needed'",
    "build": "../../bs/build.sh",
    "extract": "../../bs/extract.sh",
    "load": "../../bs/load.sh",
    "unload": "../../bs/unload.sh",
    "shell": "../../bs/run_cmd.sh bash",
    "start": "../../bs/start.sh --expose 19595 -p 19595:19595 --net \"host\" -d",
    "start-dev": "node . --bus amqp://localhost/codefarm",
    "start-rel": "npm run start-dev",
    "stop": "../../bs/stop.sh",
    "lint": "git ls-files \"*.js\" | xargs eslint"
  },
  "author": "Marcus Christensson <marcus.christensson@combitech.se>",
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  },
  "libraries": [
    "log",
    "misc",
    "auth",
    "msgbus",
    "service",
    "web",
    "typelib",
    "database",
    "servicecom",
    "singleton"
  ],
  "dependencies": {
    "service": "file:../../lib/service",
    "database": "file:../../lib/database",
    "msgbus": "file:../../lib/msgbus",
    "typelib": "file:../../lib/typelib",
    "web": "file:../../lib/web",
    "misc": "file:../../lib/misc",
    "singleton": "file:../../lib/singleton",
    "testsupport": "file:../../lib/testsupport",
    "loglib": "file:../../lib/loglib",
    "servicecom": "file:../../lib/servicecom",
    "auth": "file:../../lib/auth",
    "deep-assign": "^2.0.0",
    "fs-extra-promise": "^0.4.1",
    "moment": "^2.18.1",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "eslint": "^3.19.0"
  }
}
