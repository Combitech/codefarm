{
  "name": "exec",
  "version": "0.0.0",
  "description": "Central Execution Service",
  "main": "index.js",
  "scripts": {
    "compile": "node  --harmony_async_await ./node_modules/.bin/webpack --config ./slave/build.js",
    "build": "../../bs/build.sh",
    "extract": "../../bs/extract.sh",
    "load": "../../bs/load.sh",
    "unload": "../../bs/unload.sh",
    "shell": "../../bs/run_cmd.sh bash",
    "start": "../../bs/start.sh --expose 19494 -p 19494:19494 -d",
    "prestart-dev": "npm run compile",
    "start-dev": "node --harmony_async_await . --bus amqp://localhost/codefarm",
    "start-rel": "node --harmony_async_await . --bus amqp://localhost/codefarm",
    "stop": "../../bs/stop.sh",
    "test": "npm run compile && mocha --harmony_async_await ./test",
    "lint": "git ls-files \"*.js\" | xargs eslint",
    "install": "if [ -n \"$gitroot\" ]; then $gitroot/src/bs/linklibs.sh; else ../../bs/linklibs.sh; fi",
    "deepinstall": "if [ -n \"$gitroot\" ]; then FORCE_INSTALL_LIBS=1 $gitroot/src/bs/linklibs.sh; else FORCE_INSTALL_LIBS=1 ../../bs/linklibs.sh; fi"
  },
  "author": "Mattias Runge <mattias.runge@combitech.se>",
  "license": "MIT",
  "engines": {
    "node": ">= 7.0.0"
  },
  "libraries": [
    "database",
    "typelib",
    "msgbus",
    "loglib",
    "emitter",
    "log",
    "service",
    "misc",
    "ssh",
    "web",
    "testsupport",
    "servicecom",
    "singleton"
  ],
  "dependencies": {
    "async-busboy": "^0.3.3",
    "babel-eslint": "^7.1.0",
    "bluebird": "^3.4.6",
    "commander": "^2.9.0",
    "fs-extra-promise": "^0.4.1",
    "jsonpath-plus": "^0.16.0",
    "moment": "^2.17.1",
    "request": "^2.78.0",
    "source-map-support": "^0.4.6",
    "webpack": "^2.2.1",
    "webpack-async-await": "^1.1.0",
    "yargs": "^6.0.0",
    "get-port": "^2.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.7.0",
    "mocha": "^3.1.0",
    "request-promise": "^4.1.1"
  }
}
