{
  "name": "coderepo",
  "version": "0.0.0",
  "description": "Code Repository Service",
  "main": "index.js",
  "scripts": {
    "compile": "echo 'No compile needed'",
    "build": "../../bs/build.sh",
    "extract": "../../bs/extract.sh",
    "load": "../../bs/load.sh",
    "unload": "../../bs/unload.sh",
    "shell": "../../bs/run_cmd.sh bash",
    "start": "../../bs/start.sh --expose 19393 -p 19393:19393 --net=\"host\" -d",
    "start-dev": "node . --bus amqp://localhost/codefarm",
    "start-rel": "npm run start-dev",
    "stop": "../../bs/stop.sh",
    "test": "mocha --recursive",
    "lint": "git ls-files \"*.js\" | xargs eslint"
  },
  "author": "Mattias Runge <mattias.runge@combitech.se>",
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  },
  "libraries": [
    "database",
    "msgbus",
    "emitter",
    "log",
    "service",
    "typelib",
    "misc",
    "ssh",
    "web",
    "backend",
    "servicecom",
    "testsupport",
    "singleton"
  ],
  "dependencies": {
    "service": "file:../../lib/service",
    "database": "file:../../lib/database",
    "msgbus": "file:../../lib/msgbus",
    "emitter": "file:../../lib/emitter",
    "typelib": "file:../../lib/typelib",
    "web": "file:../../lib/web",
    "misc": "file:../../lib/misc",
    "backend": "file:../../lib/backend",
    "testsupport": "file:../../lib/testsupport",
    "servicecom": "file:../../lib/servicecom",
    "singleton": "file:../../lib/singleton",
    "ssh": "file:../../lib/ssh",
    "log": "file:../../lib/log",
    "crypto": "^0.0.3",
    "fs-extra-promise": "^0.4.1",
    "git-list-pack": "^0.0.10",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-route": "^3.2.0",
    "moment": "^2.18.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "get-port": "^3.1.0",
    "mocha": "^3.2.0"
  }
}
