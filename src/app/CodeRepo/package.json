{
  "name": "coderepo",
  "version": "0.0.0",
  "description": "Code Repository Service",
  "main": "index.js",
  "scripts": {
    "compile": "echo 'No compile needed'",
    "build": "../../bs/build.sh",
    "extract": "../../bs/extract.sh",
    "load": "../../bs/load.sh",
    "unload": "../../bs/unload.sh",
    "shell": "../../bs/run_cmd.sh bash",
    "start": "../../bs/start.sh --expose 19393 -p 19393:19393 --net=\"host\" -d",
    "start-dev": "node --harmony_async_await . --bus amqp://localhost/codefarm",
    "start-rel": "npm run start-dev",
    "stop": "../../bs/stop.sh",
    "test": "mocha --harmony_async_await --recursive",
    "lint": "git ls-files \"*.js\" | xargs eslint",
    "install": "if [ -n \"$gitroot\" ]; then $gitroot/src/bs/linklibs.sh; else ../../bs/linklibs.sh; fi",
    "deepinstall": "if [ -n \"$gitroot\" ]; then FORCE_INSTALL_LIBS=1 $gitroot/src/bs/linklibs.sh; else FORCE_INSTALL_LIBS=1 ../../bs/linklibs.sh; fi"
  },
  "author": "Mattias Runge <mattias.runge@combitech.se>",
  "license": "MIT",
  "engines": {
    "node": ">= 7.0.0"
  },
  "libraries": [
    "database",
    "msgbus",
    "emitter",
    "log",
    "service",
    "typelib",
    "misc",
    "ssh",
    "web",
    "backend",
    "servicecom",
    "testsupport",
    "singleton"
  ],
  "dependencies": {
    "crypto": "^0.0.3",
    "fs-extra-promise": "^0.4.1",
    "git-list-pack": "^0.0.10",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-route": "^3.2.0",
    "moment": "^2.18.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "get-port": "^3.1.0",
    "mocha": "^3.2.0"
  }
}
